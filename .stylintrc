{
  "maxErrors": 10,
  "maxWarnings": false,

  "universal": {
    "expect": "never",
    "error": true
  },
  "zeroUnits": {
    "expect": "never",
    "error": true
  },
  "zIndexNormalize": {
    "expect": 10,
    "error": true
  },
  "efficient": {
    "expect": "always",
    "error": false
  },
  "leadingZero": {
    "expect": "always",
    "error": true
  },
  "stackedProperties": {
    "expect": "never",
    "error": true
  },
  "semicolons": {
    "expect": "never",
    "error": true
  },
  "colons": {
    "expect": "never",
    "error": false
  },
  "brackets": {
    "expect": "never",
    "error": true
  },
  "indentPref": false,
  "mixed": {
    "expect": true,
    "error": false
  },
  "trailingWhitespace": {
    "expect": "never",
    "error": true
  },
  "parenSpace": {
    "expect": "never",
    "error": true
  },
  "commentSpace": {
    "expect": "always",
    "error": true
  },
  "quotePref": {
    "expect": "single",
    "error": false
  },
  "commaSpace": {
    "expect": "always",
    "error": true
  },
  "prefixVarsWithDollar": {
    "expect": "always",
    "error": true
  },
  "cssLiteral": {
    "expect": "never",
    "error": true
  },

  "valid": {
    "expect": true,
    "error": false
  },
  "mixins": false,
  "none": false,
  "noImportant": false,
  "depthLimit": false,
  "duplicates": false,
  "globalDupe": false,
  "namingConvention": false,
  "namingConventionStrict": false,
  "extendPref": false,
  "placeholders": false,
  "blocks": false,
  "sortOrder": false,
  "colors": false
}
